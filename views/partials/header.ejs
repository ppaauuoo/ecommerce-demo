  <body>
    <form action="/cart" method="POST">
      <!-- Fixed navbar -->
      <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container">
          <ul class="nav navbar-nav navbar-left">
            <!-- <li class="active"><a href="./">Cart <span class="sr-only"></span></a></li> -->
            <li class="col">
              <a href="/"> Home<span class="sr-only"></span></a>
            </li>
          </ul>
          <ul class="nav navbar-nav navbar-right">
            <!-- <li class="active"><a href="./">Cart <span class="sr-only"></span></a></li> -->

            <li class="col">
              <a href="./money"
                ><%=wallet[0].money%> บาท <span class="sr-only"></span
              ></a>
            </li>
            <li class="col">
              <a href="./money"
                ><%=wallet[0].point%> point <span class="sr-only"></span
              ></a>
            </li>
            <li class="col">
              <a href="./cart"
                >Cart ( <%=wallet[0].total%> บาท ) <span class="sr-only"></span
              ></a>
            </li>
          </ul>
        </div>
      </nav>
